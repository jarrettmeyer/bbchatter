<table class="table table-condensed">
	<thead>
		<tr>
			<th>ID</th>
			<th>Room Name</th>
			<th>Room Key</th>
		</tr>
	</thead>
	<tbody>
		<% @chatrooms.each do |c| %>
			<tr>
				<td> <%= c.id %> </td>
				<td> <%= c.room_name %> </td>
				<td> <%= c.room_key %> </td>
			</tr>
		<% end %>
	</tbody>
</table>